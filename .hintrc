{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "button-type": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "min-height: auto",
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "no-inline-styles": "off",
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "detect-css-reflows/layout": "off"
  }
}