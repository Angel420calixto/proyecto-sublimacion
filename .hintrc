{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "button-type": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "min-height: auto",
          "-webkit-overflow-scrolling",
          "scrollbar-width",
          "scrollbar-width: thin",
          "scrollbar-width: none",
          "scrollbar-color",
          "user-select",
          "-webkit-user-drag",
          "image-rendering: crisp-edges",
          "mask-composite",
          "mask-composite: exclude"
        ]
      }
    ],
    "no-inline-styles": "off",
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "detect-css-reflows/layout": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off",
        "duplicate-id": "off"
      }
    ],
    "disown-opener": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "video[controlslist]",
          "video[playsinline]"
        ]
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off"
      }
    ]
  }
}